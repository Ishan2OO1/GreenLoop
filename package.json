{
  "name": "ml-app-dev-runner",
  "version": "1.0.0",
  "description": "Development runner for ML Flask + React app",
  "scripts": {
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend-flask && python app.py",
    "start:frontend": "cd frontend-react && npm start",
    "install-all": "cd backend-flask && pip install -r requirements.txt && cd ../frontend-react && npm install",
    "test-api": "cd backend-flask && python comprehensive_test.py"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": ["ml", "flask", "react", "development"],
  "author": "",
  "license": "MIT"
}